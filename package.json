    "graphql",
    "pothos",
    "schema.graphql"
  ],
  "author": "SoraKumo",
  "devDependencies": {
    "@pothos/core": "^3.38.0",
    "@types/node": "^20.7.0",
    "cpy-cli": "^5.0.0",
    "graphql": "^16.8.1",
    "typescript": "^5.2.2"
  },
  "repository": "https://github.com/node-libraries/pothos-schema-exporter"
}
